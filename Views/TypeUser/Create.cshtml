@model Barbearia.Models.TypeUser

@{
    ViewData["Title"] = "Create";
}

<h1>Criar um tipo de usuário</h1>


<hr />
<div>
    <a asp-action="Index" class="text-black-50">Voltar</a>
</div>
<div class="Geral">
    <div class="col-md-4 containerForm">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TypeNameUser" class="control-label"></label>
                <input asp-for="TypeNameUser" class="form-control" />
                <span asp-validation-for="TypeNameUser" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Criar" class="btn btn-success mt-2" />
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
