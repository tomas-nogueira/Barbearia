@model IEnumerable<Barbearia.Models.User>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NameUser)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TelUser)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmailUser)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CpfUser)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CnpjUser)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TypeUser)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SenhaUser)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NameUser)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TelUser)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmailUser)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CpfUser)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CnpjUser)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TypeUser.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SenhaUser)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
